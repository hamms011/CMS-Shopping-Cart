@model IEnumerable<TheCakeShop.Models.ViewModels.Pages.PageVM>

@{
    string tempPageTitle = (string)TempData["PageTitle"];
}

@foreach (var item in Model)
{

    if (tempPageTitle == item.Title)
    {
        <li class="dropdown active"><a href="/@item.Slug">@item.Title</a></li>
    }
    else
    {
        <li class="dropdown"><a href="/@item.Slug">@item.Title</a></li>
    }

}


